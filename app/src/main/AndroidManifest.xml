<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nr12">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_nr12"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_nr12"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ListRelatorioActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PontoPerigoActivity"
            android:label="@string/title_activity_ponto_perigo" />
        <activity
            android:name=".ConfigActivity"
            android:label="@string/title_activity_cad_usuario" />
        <activity
            android:name=".CadRelatorioActivity"
            android:label="@string/title_activity_cad_relatorio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CadMaquinaActivity"
            android:label="@string/title_activity_cad_maquina" />
        <activity
            android:name=".SelecionaClienteActivity"
            android:label="@string/title_activity_seleciona_cliente"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SelecionaMaquinaActivity"
            android:label="@string/title_activity_seleciona_maquina"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>